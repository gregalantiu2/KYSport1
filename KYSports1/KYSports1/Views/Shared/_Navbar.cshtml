@model KYSports1.Models.ViewModels.ArticlePage
@{
    ViewBag.Title = "_Navbar";
}
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
    @Styles.Render("~/Content/NewArticle.css")
    @Styles.Render("~/Content/AdminList.css")
    <link href="https://fonts.googleapis.com/css?family=Anton|Cuprum|Yanone+Kaffeesatz" rel="stylesheet">
</head>
<body>
    @{
        Html.RenderPartial("~/Views/Home/NavBar.cshtml");
    }
    <div class="col-lg-12">
        @RenderBody()
    </div>
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>

